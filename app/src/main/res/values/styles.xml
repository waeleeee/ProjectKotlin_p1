<?xml version="1.0" encoding="utf-8"?>
<resources>



    <style name="EditTextStyle">
        <item name="android:padding">16dp</item>

        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#666666</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:background">@drawable/edittext_border</item>

    </style><style name="amount">



    <item name="android:textSize">16sp</item>
    <item name="android:padding">16dp</item>
    <item name="android:textColor">#000000</item>
    <item name="android:textColorHint">#666666</item>


    <item name="android:background">@drawable/edittext_border</item>
</style>


    <style name="border">
        <item name="android:background">@drawable/edittext_border</item>
    </style>

    <style name="nothing" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemActiveIndicatorStyle">@null</item>
    </style>


    <style name="header">

        <item name="android:textSize">32sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="active">

        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#333333</item>

    </style>
    <style name="inactive">

        <item name="android:textSize">11sp</item>

    </style>

    <style name="btn">
        <item name="android:padding">16sp</item>

        <item name="android:backgroundTint">@color/btn</item>

    </style>

    <style name="errorM">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#FFFF0000</item>



    </style>
    <style name="logo">
        <item name="android:letterSpacing">0.07</item>
        <item name="android:textSize">64sp</item>

    </style>


    <style name="LabelStyle">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginBottom">4sp</item>
    </style>
</resources>